@model IEnumerable<AdvisingServices.Models.Advisor>
@{
    ViewBag.Title = "Advisors";
}
<div class="container">
    <h1 class="page-header">Advisors</h1>
    <table class="table table-hover">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Department</th>
                <th>Avalability</th>
            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Fname</td>
                <td>@item.Lname</td>
                <td>@item.Department</td>
                @if (item.freedom == true)
                {
                  <td><img style="height:20px" src="~/Images/greenDot.png"/></td>
                }
                else { <td><img style="height:20px" src="~/Images/redDot.png"/></td>}

            </tr>
        }
    </table>
</div>